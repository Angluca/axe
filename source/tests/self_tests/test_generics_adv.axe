use std.io;

model SomeModel {
    pub def some_function[T](arg: T): T {
        when T is i32 {
            return arg + 1;
        }
        when T is f32 {
            return arg * 2.0;
        }
    }
}

def main() {
    println(SomeModel.some_function[i32](5));
    println(SomeModel.some_function[f32](5.0));
}