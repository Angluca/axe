def randomize(): void {
    raw {
        srand(time(NULL));
    }
}

def random(a: i32, b: i32): i32 {
    randomize();
    raw {
        return rand() % (b - a + 1) + a;
    }
}

test {
    assert(random(1, 10) >= 1, "Random number should be between 1 and 10");
    assert(random(1, 10) <= 10, "Random number should be between 1 and 10");
}